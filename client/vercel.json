{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install && chmod 755 node_modules/.bin/vite && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --production=false",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
} 